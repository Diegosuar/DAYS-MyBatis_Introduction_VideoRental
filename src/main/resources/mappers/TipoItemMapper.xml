<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
  PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
  "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
  
<mapper namespace="edu.unisabana.dyas.sampleprj.dao.mybatis.mappers.TipoItemMapper">


<select parameterType="map" id="getTipoItems" resultMap="TipoItemResult">
select
ti.id AS tipoitem_id,
ti.descripcion AS tipoitem_descripcion
FROM VI_TIPOITEM AS ti
</select>

<resultMap type="TipoItem" id="TipoItemResult">
  <id property="id" column="ID_TIPO_ITEM"/>
  <result property="descripcion" column="tipoitem_descripcion"/>
</resultMap>
                
</mapper>